{
    "name": "gpupo/cnova-sdk",
    "description": "SDK Não Oficial para integração a partir de aplicações PHP com as APIs da Cnova Marketplace",
    "homepage": "https://opensource.gpupo.com/cnova-sdk/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "keywords": ["cnova","extra", "pontofrio", "casasbahia", "sdk", "api"],
    "license": "MIT",
    "require": {
        "php": "^5.6 || ^7.0",
        "ext-curl" : "*",
        "symfony/console": "3.3.10",
        "gpupo/cache": "1.3.2",
        "gpupo/common": "1.7.7",
        "gpupo/common-sdk": "2.2.15"
    },
    "autoload": {
        "psr-4": {
            "Gpupo\\CnovaSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gpupo\\Tests\\CnovaSdk\\": "tests"
        }
    },
    "scripts": {
        "pre-install-cmd": [
            "touch Resources/logs/tests.log"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
